{
  "welcomeFile": "/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/odata/v4/EmpleadosService/(.*)$",
      "target": "/odata/v4/EmpleadosService/$1",
      "destination": "srv-api",
      "csrfProtection": false
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "localDir": "app/webapp"
    }
  ]
}
